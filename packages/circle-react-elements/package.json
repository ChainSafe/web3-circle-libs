{
  "name": "@circle-libs/react-elements",
  "version": "0.1.0",
  "description": "React components compatible with Circle SDK",
  "keywords": [
    "react",
    "components",
    "circle",
    "usdc",
    "stablecoins",
    "programmable wallets"
  ],
  "homepage": "https://github.com/ChainSafe/web3-circle-libs#readme",
  "bugs": {
    "url": "https://github.com/ChainSafe/web3-circle-libs/issues"
  },
  "license": "MIT",
  "author": "ChainSafe",
  "contributors": [
    "ChainSafe <info@chainsafe.io>"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChainSafe/web3-circle-libs.git"
  },
  "scripts": {
    "build": "yarn build:js && yarn build:css",
    "build:js": "tsup",
    "build:css": "tailwindcss -i src/styles/tailwind.css -o dist/styles.css",
    "lint": "tsc --noEmit && eslint 'src/**/*.{ts,tsx}'",
    "watch": "tsup --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@web3icons/react": "^4.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "ethereum-blockies-base64": "^1.0.2",
    "react-hook-form": "^7.54.2",
    "qrcode.react": "^4.1.0",
    "tailwind-merge": "^3.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/preview-api": "^8.5.2",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@storybook/theming": "^8.5.2",
    "@tailwindcss/cli": "^4.0.4",
    "@tailwindcss/vite": "^4.0.4",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.17.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.1",
    "prettier": "^3.4.1",
    "storybook": "^8.5.0",
    "storybook-dark-mode": "^4.0.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.2.0"
  },
  "peerDependencies": {
    "@circle-fin/developer-controlled-wallets": "~7.1.0",
    "lucide-react": ">=0.474.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "tailwindcss": ">=4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
