{
  "name": "circle-react-components",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChainSafe/web3-circle-libs/"
  },
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
